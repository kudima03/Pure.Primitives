<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <IsTrimmable>true</IsTrimmable>
    <EnableTrimAnalyzer>true</EnableTrimAnalyzer>
    <EnableAotAnalyzer>true</EnableAotAnalyzer>
  </PropertyGroup>
  <ItemGroup>
    <InternalsVisibleTo Include="Pure.Primitives.Tests" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.NET.ILLink.Tasks" Version="9.0.10" />
    <PackageReference Include="Pure.Primitives.Abstractions" Version="4.0.11" />
  </ItemGroup>
  <PropertyGroup>
    <PackageId>Pure.Primitives</PackageId>
    <RootNamespace>Pure.Primitives</RootNamespace>
    <AssemblyName>Pure.Primitives</AssemblyName>
    <Authors>Dmitry Kurochkin</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/kudima03/Pure.Primitives</PackageProjectUrl>
  </PropertyGroup>
</Project>
